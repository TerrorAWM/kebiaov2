[
    {
        "id": "auto-hit",
        "name": "HIT",
        "csv": {
            "encoding": "utf-8",
            "header_row_must_include": [
                "星期一",
                "星期二",
                "星期三",
                "星期四",
                "星期五",
                "星期六",
                "星期日"
            ],
            "title_rows_to_skip": 1,
            "section_col": 0,
            "period_col": 1,
            "day_cols_from": 2,
            "day_cols_count": 7,
            "period_pattern": "第(?<nums>[0-9,，\\-、]+)节",
            "split_cell_by": "\\n+",
            "skip_tokens": [
                "",
                "-",
                "—",
                "— —",
                "— — —"
            ],
            "detail_line_regex": "^(?<teacher>[^\\[]+?)\\[(?<weeks>[^\\]]+)\\]周(?<room>.*)$",
            "room_fallback_keywords": [
                "正心",
                "致知",
                "格物",
                "理学",
                "管理",
                "电机",
                "机电",
                "主楼",
                "机械",
                "G",
                "B",
                "成教",
                "一校区",
                "二校区",
                "校区",
                "楼",
                "教室"
            ],
            "normalize": {
                "fullwidth_to_halfwidth": true,
                "replace_cn_comma_to_en": true,
                "replace_cn_dash_to_en": true
            },
            "week_type_rules": [
                {
                    "contains": "单周",
                    "week_type": "odd"
                },
                {
                    "contains": "双周",
                    "week_type": "even"
                }
            ],
            "ai": false,
            "ai_prompt": ""
        }
    },
    {
        "id": "hit-manual-txt",
        "name": "HIT-TXT ",
        "csv": {
            "encoding": "utf-8",
            "manual_txt": true,
            "day_token_regex": "^\\s*(星期[一二三四五六日])\\s*$",
            "period_token_regex": "^\\s*(\\d{1,2}(?:-\\d{1,2}|(?:,\\d{1,2})*))\\s*$",
            "detail_line_regex": "^(?<teacher>[^\\[]+?)\\[(?<weeks>[^\\]]+)\\]周(?<room>.*)$",
            "normalize": {
                "fullwidth_to_halfwidth": true,
                "replace_cn_comma_to_en": true,
                "replace_cn_dash_to_en": true
            },
            "week_type_rules": [
                {
                    "contains": "单周",
                    "week_type": "odd"
                },
                {
                    "contains": "双周",
                    "week_type": "even"
                }
            ],
            "room_fallback_keywords": [
                "正心",
                "致知",
                "格物",
                "理学",
                "管理",
                "电机",
                "机电",
                "主楼",
                "机械",
                "G",
                "B",
                "成教",
                "成教楼",
                "一校区",
                "二校区",
                "校区",
                "楼",
                "教室"
            ],
            "ai": false,
            "ai_prompt": ""
        }
    }
]